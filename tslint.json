{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [true, "log"],
        "no-empty": false,
        "max-line-length": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "curly": false,
        "member-access": false,
        "no-string-literal": false,
        "no-any": true
    },
    "rulesDirectory": []
}